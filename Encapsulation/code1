package oops_program;

public class encap1 
{          						//alt+shift+s         to generate getters & setters
	private String name;
	private int age;
	
	public void setname(String name1)
	{
		name=name1;
	}
	public void setage(int age1)
	{
		age=age1;
	}
	
	public String getname()
	{
		return name;
	}
	public int getage()
	{
		return age;
	}
}
